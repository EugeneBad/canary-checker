{
  "name": "canary-checker",
  "version": "0.1.0",
  "dependencies": {
    "@flanksource/flanksource-ui": "^1.0.98"
  },
  "proxy": "https://canaries.canary.lab.flanksource.com/",
  "scripts": {
    "start": "node ./scripts/serve-build.js",
